{
  "name": "common-middleware",
  "description": "Common middleware for applications built with base-methods (like assemble, verb, generate, and update)",
  "version": "1.0.0",
  "homepage": "https://github.com/jonschlinkert/common-middleware",
  "author": "Jon Schlinkert (https://github.com/jonschlinkert)",
  "repository": "jonschlinkert/common-middleware",
  "bugs": {
    "url": "https://github.com/jonschlinkert/common-middleware/issues"
  },
  "license": "MIT",
  "files": [
    "index.js"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=0.10.0"
  },
  "scripts": {
    "test": "mocha"
  },
  "dependencies": {
    "ansi-colors": "^3.0.5",
    "debug": "^2.6.8",
    "define-property": "^1.0.0",
    "file-is-binary": "^1.0.0",
    "is-valid-app": "^0.3.0",
    "isobject": "^3.0.1",
    "middleware-rename-file": "^1.0.0",
    "middleware-utils": "^1.0.0",
    "mixin-deep": "^1.2.0",
    "parser-front-matter": "^1.6.4"
  },
  "devDependencies": {
    "engine-base": "^0.1.3",
    "gulp-format-md": "^1.0.0",
    "mocha": "^3.4.2",
    "templates": "^1.2.8"
  },
  "keywords": [
    "common",
    "front",
    "front-matter",
    "matter",
    "middleware",
    "parse",
    "templates",
    "yaml"
  ],
  "verb": {
    "run": true,
    "toc": false,
    "layout": "default",
    "data": {
      "author": {
        "username": "jonschlinkert"
      }
    },
    "tasks": [
      "readme"
    ],
    "plugins": [
      "gulp-format-md"
    ],
    "related": {
      "list": [
        "assemble-core",
        "base",
        "generate",
        "update",
        "verb"
      ]
    },
    "reflinks": [
      "verb",
      "verb-readme-generator"
    ],
    "lint": {
      "reflinks": true
    }
  }
}
